<%- include('../partials/navbar.ejs')%>
<%- include('../partials/sidebar_right.ejs')%>
<%- include('../partials/supAdmin_sidebar')%>


<div class="main-container">
	<div class="pd-ltr-20 xs-pd-20-10">
		<div class="min-height-200px">
			<div class="page-header">
				<div class="row">
					<div class="col-md-6 col-sm-12">
						<div class="title">
							<h4>Subscriber List</h4>
						</div>
						<nav aria-label="breadcrumb" role="navigation">
							<ol class="breadcrumb">
								<li class="breadcrumb-item"><a href="index.html">Home</a></li>
								<li class="breadcrumb-item active" aria-current="page">Verification List</li>
							</ol>
						</nav>
					</div>

				</div>
			</div>




			<!-- <div class="card-box mb-30" style="float: right; padding-right: 20px; padding-top: 20px;">
                <a href="/user/create">
                    <button type="button" class="btn btn-primary">Primary</button>
                </a>
             </div> -->



			<div class="card-box mb-30">
				<div class="pd-20">
					<div class="clearfix">
						<div class="pull-left">
							<h4 class="text-blue h4">Super Admin List</h4>

						</div>

					</div>

				</div>

				<div class="pb-20">

					<table class="table hover data-table  nowrap ">
						<thead>
							<tr>
								<th class="table-plus datatable-nosort">ID</th>
								<th>Subscription</th>
								<th>CompName</th>
								<th>Email</th>
								<th>Contact</th>
								
								
								<th>Manager Name</th>
						
								<th class="datatable-nosort">Action</th>
								
								<th>Address</th>
								<th>Total Employee</th>
							</tr>
						</thead>
						<tbody>
							<% for(var i=0; i< userlist.length; i++ ){ %>
							<tr>
								<td class="table-plus"><%= userlist[i].id %></td>
								<td><%= userlist[i].Subscription_Type %></td>
								<td><%= userlist[i].Company_Name %></td>
								<td><%= userlist[i].Company_Email %></td>
								<td><%= userlist[i].Contact_No %></td>
								
							
								<td><%= userlist[i].Manager_Name %></td>
								
								<td>


									<div class="dropdown">
										<a class="btn btn-link font-24 p-0 line-height-1 no-arrow dropdown-toggle"
											href="#" role="button" data-toggle="dropdown">
											<i class="dw dw-more"></i>
										</a>
										<div class="dropdown-menu dropdown-menu-right dropdown-menu-icon-list">
											
											<a class="dropdown-item" href="/supAdmin_home/verification/verify/<%=userlist[i].id%>"><i class="dw dw-edit2"></i> Verify</a>
											<!-- <a class="dropdown-item" href="/supAdmin_home/verification/delete/<%=userlist[i].id%>"><i class="dw dw-delete-3"></i> Delete</a> -->
										</div>
									</div>

								</td>
								
								<td><%= userlist[i].Company_Address %></td>
								<td><%= userlist[i].Total_Employee %> </td>
							</tr>
							<% } %>

						</tbody>
					</table>

				</div>
			</div>
		</div>
	</div>
</div>